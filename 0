repeat wait(5) until game:IsLoaded()

spawn(function()
    local scriptUrls = {
        'https://raw.githubusercontent.com/vouvy/CheckAFK/main/0',
        'https://raw.githubusercontent.com/vouvy/CheckMelee/main/0'
    }
    for _, url in ipairs(scriptUrls) do
        local success, result = pcall(function()
            return loadstring(game:HttpGetAsync(url))()
        end)
        
        if not success then
            warn("Failed to load script from URL: " .. url .. "\nError: " .. result)
        end
    end
end)
